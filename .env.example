# Example environment variables for OAuth and Firebase
# Copy this file to .env (or inject using EAS secrets / CI env vars)

# Google OAuth client IDs
# Web client id (used for web sign-in)
EXPO_GOOGLE_WEB_CLIENT_ID=your-web-client-id.apps.googleusercontent.com

# Android client id (OAuth client created for package com.datafusion.wishlane)
EXPO_GOOGLE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com

# iOS client id (if you build for iOS)
EXPO_GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com

# Optional: control whether to use Expo proxy during development (default: true in Expo Go)
EXPO_USE_PROXY=true
